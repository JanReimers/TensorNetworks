<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ITensor" />
		<Option makefile_is_custom="1" />
		<Option execution_dir="ITensor" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<MakeCommands>
			<Build command="$make " />
			<CompileFile command="$make -f $makefile $file" />
			<Clean command="$make clean" />
			<DistClean command="$make -f $makefile distclean$target" />
			<AskRebuildNeeded command="$make -q -" />
			<SilentBuild command="$make  &gt; $(CMD_NULL)" />
		</MakeCommands>
		<Build>
			<Target title="Debug">
				<Option output="ITensor/lib/itensor-g" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/Debug/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
				</Compiler>
				<MakeCommands>
					<Build command="$make " />
					<CompileFile command="$make $file" />
					<Clean command="$make clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q lib/libitensor-g.a" />
					<SilentBuild command="$make  &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
			<Target title="Release">
				<Option output="ITensor/lib/itensor" prefix_auto="1" extension_auto="1" />
				<Option working_dir="" />
				<Option object_output="obj/Release/" />
				<Option type="2" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Compiler>
					<Add option="-O2" />
					<Add option="-Wall" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
				<MakeCommands>
					<Build command="$make " />
					<CompileFile command="$make $file" />
					<Clean command="$make clean" />
					<DistClean command="$make -f $makefile distclean$target" />
					<AskRebuildNeeded command="$make -q lib/libitensor.a" />
					<SilentBuild command="$make  &gt; $(CMD_NULL)" />
				</MakeCommands>
			</Target>
		</Build>
		<ExtraCommands>
			<Add before="if [ ! -f options.mk ]; then cp ../ITensor-options.mk options.mk; fi" />
		</ExtraCommands>
		<Unit filename="ITensor/Makefile" />
		<Unit filename="ITensor/options.mk" />
		<Extensions>
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
